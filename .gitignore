.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Log files
*.log

# Build directories
build/
dist/

# Temporary files
*.tmp
*.temp
.cache/
output/